#include <iostream>
using namespace std;

class Employee {
  private:
    int salary;

  public:
    void setSalary(int s) {
      salary = s;
    }
    int getSalary() {
      return salary;
    }
};

int main() {
  Employee salariu;
  salariu.setSalary(50000);
  cout << salariu.getSalary();
  return 0;
}
